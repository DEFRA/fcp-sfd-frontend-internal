{
  "name": "fcp-sfd-frontend-internal",
  "version": "0.60.0",
  "description": "SFD Frontend",
  "sideEffects": false,
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "run-p frontend:watch server:watch",
    "dev:debug": "run-p frontend:watch server:debug",
    "frontend:watch": "NODE_ENV=development webpack --watch",
    "postinstall": "npm run build",
    "lint": "run-s test:lint lint:scss",
    "lint:fix": "standard . --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "pretest": "npm run test:lint",
    "docker:debug": "docker compose -f compose.yaml -f compose.debug.yaml -p \"fcp-sfd-frontend-internal\" up",
    "docker:dev": "docker compose up",
    "docker:test": "npm run lint && mkdir -p coverage 2>/dev/null || true docker compose -p fcp-sfd-frontend-internal-test down -v && docker compose -f compose.yaml -f compose.test.yaml -p fcp-sfd-frontend-internal-test run --build --rm fcp-sfd-frontend-internal",
    "docker:test:watch": "docker compose -p fcp-sfd-frontend-internal-test down -v && docker compose -f compose.yaml -f compose.test.yaml -f compose.test.watch.yaml -p fcp-sfd-frontend-internal-test run --build --rm fcp-sfd-frontend-internal",
    "test": "rm -rf ./coverage/** && vitest run --coverage",
    "test:watch": "vitest watch",
    "test:lint": "standard",
    "start:debug": "nodemon --watch src --exec 'node --experimental-vm-modules --inspect-brk=0.0.0.0 src/index.js'",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,njk --legacy-watch src/index.js",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js --legacy-watch src/index.js",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict ."
  },
  "author": "Defra DDTS",
  "contributors": [
    "Amirs Ishmuhametovs <amirs.ishmuhametovs@defra.gov.uk>",
    "Fay Toward <fay.toward@defra.gov.uk>",
    "Rana Salem <rana.salem@defra.gov.uk>",
    "Abidemi Adio <abidemi.adio@defra.gov.uk>",
    "John Watson <john.watson1@defra.gov.uk>",
    "Rebecca Ransome <rebecca.ransome@defra.gov.uk>",
    "Simon Dunn <simon.dunn1@defra.gov.uk>",
    "Adam Kay <adam.kay@defra.gov.uk>"
  ],
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/hapi-tracing": "1.0.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/bell": "13.1.0",
    "@hapi/catbox-memory": "6.0.2",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/cookie": "12.0.1",
    "@hapi/crumb": "9.0.1",
    "@hapi/hapi": "21.3.12",
    "@hapi/inert": "7.1.0",
    "@hapi/jwt": "3.2.0",
    "@hapi/scooter": "7.0.0",
    "@hapi/vision": "7.0.3",
    "@hapi/wreck": "18.1.0",
    "@hapi/yar": "11.0.2",
    "aws-embedded-metrics": "4.2.0",
    "blankie": "5.0.0",
    "cheerio": "1.1.0",
    "convict": "6.2.4",
    "cssnano": "7.0.6",
    "cssnano-preset-default": "7.0.6",
    "date-fns": "4.1.0",
    "global-agent": "3.0.0",
    "govuk-frontend": "5.7.1",
    "graphql": "^16.11.0",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "http-status-codes": "2.3.0",
    "https-proxy-agent": "7.0.6",
    "ioredis": "5.4.1",
    "jwk-to-pem": "2.0.7",
    "nunjucks": "3.2.4",
    "pino": "9.5.0",
    "pino-pretty": "13.0.0",
    "undici": "6.21.3"
  },
  "devDependencies": {
    "@types/convict": "6.1.6",
    "@types/hapi__catbox-memory": "4.1.8",
    "@types/node": "22.9.0",
    "@types/nunjucks": "3.2.6",
    "@types/webpack-assets-manifest": "5.1.4",
    "@vitest/coverage-v8": "3.1.3",
    "autoprefixer": "10.4.20",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "eslint": "9.29.0",
    "jsdom": "26.1.0",
    "neostandard": "0.12.1",
    "nodemon": "3.1.7",
    "npm-run-all": "4.1.5",
    "postcss-load-config": "6.0.1",
    "postcss-loader": "8.1.1",
    "sass-embedded": "1.80.6",
    "sass-loader": "16.0.3",
    "source-map-loader": "5.0.0",
    "standard": "17.1.2",
    "stylelint": "16.10.0",
    "stylelint-config-gds": "2.0.0",
    "terser-webpack-plugin": "5.3.10",
    "vitest": "3.1.3",
    "webpack": "5.96.1",
    "webpack-assets-manifest": "5.2.1",
    "webpack-cli": "5.1.4"
  }
}